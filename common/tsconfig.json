{
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
    },
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "outDir": "dist"
  },
  "watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents"
  },
  "exclude": [
    "node_modules"
  ],
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}
