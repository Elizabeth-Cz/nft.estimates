{
  "rules": {
    "no-unused-vars": 1,
    "no-mixed-spaces-and-tabs": "off",
    "react/display-name": "off",
    "no-empty-pattern": "off",
    "react-hooks/rules-of-hooks": "off"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": ["react"]
}
